{
  "name": "azure-blob-storage-file-explorer-react-app",
  "description": "Veritas UI",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "release": "npx -y semantic-release --no-verify",
    "pre-commit": "npm run lint && npm run format",
    "commit": "cz && npm run release && git push --follow-tags",
    "prepare": "husky install",
    "test": "jest --coverage"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "semanticReleaseNotes": false
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
